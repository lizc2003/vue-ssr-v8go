{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "typecheck": "vue-tsc --noEmit",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vue-tsc -b && vite build --ssrManifest --outDir ../dist/public --emptyOutDir",
    "build:server": "vue-tsc -b && vite build -c vite.config.server.ts --outDir ../dist/server --emptyOutDir",
    "preview": "vite preview"
  },
  "dependencies": {
    "@unhead/vue": "^2.0.8",
    "axios": "^1.9.0",
    "pinia": "^3.0.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/runtime-core": "^3.5.13",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "sass-embedded": "^1.87.0",
    "typescript": "~5.8.3",
    "vite": "^6.3.4",
    "vue-tsc": "^2.2.10"
  }
}
